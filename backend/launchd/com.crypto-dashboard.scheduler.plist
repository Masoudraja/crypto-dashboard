<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.crypto-dashboard.scheduler</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Volumes/Masoud Case Sensetive/crypto-dashboard/venv/bin/python</string>
        <string>/Volumes/Masoud Case Sensetive/crypto-dashboard/task_scheduler.py</string>
        <string>--daemon</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Volumes/Masoud Case Sensetive/crypto-dashboard</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/Volumes/Masoud Case Sensetive/crypto-dashboard/logs/scheduler.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Volumes/Masoud Case Sensetive/crypto-dashboard/logs/scheduler_error.log</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>